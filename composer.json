{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "graham-campbell/markdown": "13.1",
    "guzzlehttp/guzzle": "^7.0.1",
    "laravel/framework": "^8.40",
    "laravel/tinker": "^2.5",
    "spatie/laravel-sluggable": "^2.6"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.6",
    "fakerphp/faker": "^1.9.1",
    "filp/whoops": "^2.13",
    "laravel/dusk": "^6.15",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "phpunit/php-code-coverage": "^9.2",
    "phpunit/phpunit": "^9.3.3"
  },
  "autoload": {
    "files": [
      "app/Helpers/Dates.php",
      "app/Helpers/Flashers.php",
      "app/Helpers/Generators.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Libs\\": "app/Libs/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "copy-env": [
      "@php -r \"copy('.env.example', '.env');\""
    ],
    "copy-env-testing": [
      "@php -r \"copy('.env.testing.example', '.env.testing');\""
    ],
    "copy-env-dusk": [
      "@php -r \"copy('.env.dusk.local.example', '.env.dusk.local');\""
    ],
    "post-install-cmd": [
      "npm i",
      "npm audit fix",
      "npm run dev",
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php -r \"file_exists('.env.dusk.local') || copy('.env.dusk.local.example', '.env.dusk.local');\"",
      "@php -r \"file_exists('.env.testing') || copy('.env.testing.example', '.env.testing');\"",
      "@php -r \"file_put_contents('database/db.sqlite', '');\"",
      "@php -r \"file_put_contents('database/dusk.sqlite', '');\"",
      "@php -r \"file_put_contents('database/testing.sqlite', '');\"",
      "@php artisan key:generate --ansi",
      "@php artisan key:generate --env=testing --ansi",
      "@php artisan key:generate --env=dusk.local --ansi",
      "@php artisan dusk:chrome-driver --all",
      "@php artisan migrate",
      "@php artisan db:seed"
    ],
    "test": [
      "vendor/bin/phpunit --verbose --coverage-html tests/reports --configuration phpunit.xml",
      "@php artisan migrate:refresh",
      "@php artisan db:seed",
      "@php artisan optimize:clear"
    ],
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@php artisan serve --port=8000"
    ],
    "coverage-ff": [
      "start firefox \"%cd%\\tests\\reports\\index.html\""
    ],
    "coverage-chrome": [
      "start chrome \"%cd%\\tests\\reports\\index.html\""
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}